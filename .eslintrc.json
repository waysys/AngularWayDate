{
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      4
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ]
  },

  "globals": {
    "WayDate": true,
    "Calendar" : true,
    "module" : false,
    "test" : false,
    "QUnit": false
  }

}